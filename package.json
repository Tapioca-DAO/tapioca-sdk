{
  "version": "1.5.6",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --transpileOnly && npm run copy-dts && npm run copy-sol && npm run copy-json && npm run copy:sandbox",
    "copy:sandbox": "cp -r dist/ sandbox/dist",
    "test": "tsdx test --passWithNoTests",
    "lint": "eslint . --fix",
    "prepare": "husky install && npm run build",
    "deploy": "npm run prepare && npm publish",
    "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" dist",
    "copy-sol": "copyfiles -u 1 \"src/**/*.sol\" dist",
    "copy-json": "copyfiles -u 1 \"src/**/*.json\" dist"
  },
  "name": "tapioca-sdk",
  "author": "0xRektora",
  "module": "dist/tapioca-sdk.esm.js",
  "dependencies": {
    "ethers": "^5.4.7",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.5",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "typechain": "^8.1.0",
    "uuid": "^9.0.0",
    "write-json-file": "4.3.0"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.4.7",
    "@ethersproject/providers": "^5.4.7",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.184",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=12.0.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "chai": "^4.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "hardhat": "^2.11.0",
    "hardhat-gas-reporter": "^1.0.8",
    "husky": "^7.0.4",
    "prettier": "^2.7.1",
    "solidity-coverage": "^0.8.1",
    "ts-node": ">=8.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": ">=4.5.0"
  },
  "peerDependencies": {
    "@nomicfoundation/hardhat-toolbox": "2.x",
    "hardhat": "2.x",
    "inquirer": "8.x"
  },
  "resolutions": {
    "tsdx/typescript": "^4.6.3"
  }
}
