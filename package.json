{
    "version": "1.3.45",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build --transpileOnly && npm run copy-dts && npm run copy-sol && npm run copy-json",
        "test": "tsdx test --passWithNoTests",
        "lint": "eslint ./**/*.ts --fix",
        "prepare": "husky install && npm run build",
        "deploy": "npm run prepare && npm publish",
        "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" dist",
        "copy-sol": "copyfiles -u 1 \"src/**/*.sol\" dist",
        "copy-json": "copyfiles -u 1 \"src/**/*.json\" dist"
    },
    "name": "tapioca-sdk",
    "author": "0xRektora",
    "module": "dist/tapioca-sdk.esm.js",
    "dependencies": {
        "ethers": "^5.6.9",
        "inquirer": "^8.2.5",
        "lodash": "^4.17.21",
        "lodash-es": "^4.17.21",
        "typechain": "^8.1.0",
        "uuid": "^9.0.0",
        "write-json-file": "4.3.0"
    },
    "devDependencies": {
        "@nomicfoundation/hardhat-toolbox": "^2.0.2",
        "@types/chai": "^4.3.3",
        "@types/inquirer": "^9.0.3",
        "@types/jest": "^28.1.6",
        "@types/lodash": "^4.14.184",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.33.0",
        "@typescript-eslint/parser": "^5.33.0",
        "chai": "^4.3.6",
        "copyfiles": "^2.4.1",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "hardhat": "^2.13.0",
        "husky": "^7.0.4",
        "prettier": "^2.7.1",
        "tsdx": "^0.14.1",
        "tslib": "^2.4.0",
        "typescript": "^4.8.2"
    },
    "peerDependencies": {
        "@nomicfoundation/hardhat-toolbox": "2.x",
        "inquirer": "8.x",
        "hardhat": "2.x"
    },
    "resolutions": {
        "tsdx/typescript": "^4.6.3"
    }
}
